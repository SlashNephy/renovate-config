{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:best-practices',
    ':label(renovate)',
    'github>SlashNephy/renovate-config//config/automerge.json5',
    'github>SlashNephy/renovate-config//config/schedule.json5',
    'github>SlashNephy/renovate-config//config/security.json5',
    'github>SlashNephy/renovate-config//config/source/docker.json5',
    'github>SlashNephy/renovate-config//config/source/github-actions.json5',
    'github>SlashNephy/renovate-config//config/source/golang.json5',
    'github>SlashNephy/renovate-config//config/source/kubernetes.json5',
  ],
  printConfig: true,
  packageRules: [
    {
      matchUpdateTypes: [
        'major'
      ],
      extends: [
        ':assignAndReview(SlashNephy)',
      ],
    },
  ],
  suppressNotifications: [
    'prIgnoreNotification',
  ],
}
